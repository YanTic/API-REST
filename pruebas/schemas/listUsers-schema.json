{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "title": "List of Users - by GetAllUsers service-handler",
    "type": "array",
    "items": {
      "type": "object",
      "properties": {
        "Id": {"type": "integer"},
        "Username": {"type": "string"},
        "Password": {"type": "string"},
        "Email": {"type": "string", "format": "email"}
      },
      "required": ["Id", "Username", "Password", "Email"]
    }
  }